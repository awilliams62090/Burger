<div class="row">
    <div class="col-6">

        {{#each burger }}
        {{#unless this.devoured}}
        <h4>Burger Name: {{this.burger_name}}</h4>
        <button class="burgButton" data-id={{this.id}}> Devour! </button>
        {{/unless}}
        {{/each}}

    </div>


    <div class="col-6">

        {{#each burger }}
        {{#if this.devoured}}
        <h4>Burger Name: {{this.burger_name}}</h4>
        {{/if}}
        {{/each}}
    </div>
</div>
<div class="row"></div>
<div class="row" id="game">
    <div class="col-4"></div>
    <div class="col-4">
        <form class="create-form">
            <div class="form-group">
                <label for="burg">Add a Burger! </label>
                <input type="text" class="form-control create-form" id="burg">
                <button type="submit">Submit!</button>
            </div>
        </form>
    </div>
    <div class="col-4"></div>
</div>